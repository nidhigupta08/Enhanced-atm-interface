<?xml version="1.0" encoding="UTF-8"?>
<element>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
         version="3.1">
    
    <display-name>EnhancedATMInterface</display-name>
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>updated.atminterface.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
      
    <servlet>
    <servlet-name>MainMenuServlet</servlet-name>
    <servlet-class>updated.atminterface.servlets.MainMenuServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>MainMenuServlet</servlet-name>
    <url-pattern>/mainMenu</url-pattern>
</servlet-mapping>
    
    <servlet>
        <servlet-name>TransactionHistoryServlet</servlet-name>
        <servlet-class>updated.atminterface.servlets.TransactionHistoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TransactionHistoryServlet</servlet-name>
        <url-pattern>/TransactionHistoryServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>WithdrawServlet</servlet-name>
        <servlet-class>updated.atminterface.servlets.WithdrawServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WithdrawServlet</servlet-name>
        <url-pattern>/WithdrawServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DepositServlet</servlet-name>
        <servlet-class>updated.atminterface.servlets.DepositServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DepositServlet</servlet-name>
        <url-pattern>/DepositServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>TransferServlet</servlet-name>
        <servlet-class>updated.atminterface.servlets.TransferServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TransferServlet</servlet-name>
        <url-pattern>/TransferServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>updated.atminterface.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
</element>
   
